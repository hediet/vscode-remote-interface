{
	"name": "vscode-rpc-server",
	"displayName": "VSCode RPC Server",
	"description": "Provides an RPC interface to automate VSCode from other processes",
	"icon": "docs/logo.png",
	"version": "0.9.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.30.0"
	},
	"publisher": "hediet",
	"author": {
		"email": "henning.dieterichs@live.de",
		"name": "Henning Dieterichs"
	},
	"readme": "./README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/hediet/vscode-rpc-server.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"title": "Open RPC Server Configuration File",
				"command": "vscode-rpc-server.open-server-config"
			},
			{
				"title": "Reload RPC Server Configuration",
				"command": "vscode-rpc-server.reload-server-config"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"@hediet/std": "^0.2.0",
		"@hediet/typed-json-rpc": "^0.5.0",
		"@hediet/typed-json-rpc-streams": "^0.5.0",
		"@hediet/typed-json-rpc-websocket": "^0.5.0",
		"@hediet/typed-json-rpc-websocket-server": "^0.5.0",
		"@types/crypto-hash": "^1.0.0",
		"@types/crypto-random-string": "^1.0.0",
		"@types/env-paths": "^1.0.2",
		"@types/get-port": "^4.0.1",
		"@types/ws": "^6.0.1",
		"crypto-hash": "^1.0.0",
		"crypto-random-string": "^1.0.0",
		"env-paths": "^2.1.0",
		"get-port": "^4.2.0",
		"io-ts": "^1.8.2",
		"io-ts-types": "^0.4.3",
		"mkdir-recursive": "^0.4.0",
		"monocle-ts": "^1.7.1",
		"newtype-ts": "^0.2.4",
		"vscode-rpc": "^0.9.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.4.0-dev.20190314",
		"vscode": "^1.1.25"
	}
}
