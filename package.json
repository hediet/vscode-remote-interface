{
	"name": "remote-interface",
	"displayName": "remote-interface",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:remote-interface.start-server"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "remote-interface.start-server",
				"title": "Start remote interface server"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"@hediet/typed-json-rpc": "^0.1.0",
		"@hediet/typed-json-rpc-websocket": "^0.1.0",
		"@hediet/typed-json-rpc-websocket-server": "^0.1.0"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
